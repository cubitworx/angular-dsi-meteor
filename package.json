{
  "name": "@cubitworx/angular-dsi-meteor",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "description": "Angular-Meteor datasource abtraction built around rxjs",
  "main": "./dist/src",
  "module": "./dist/src",
  "typings": "./dist/src",
  "scripts": {
    "clean": "rimraf ./build && rimraf ./dist",
    "copy": "copyfiles ./LICENSE ./README.md ./package-dist.json ./dist && renamer --find package-dist.json --replace package.json ./dist/*",
    "build": "npm run clean && ngc -p ./tsconfig-dist.json && npm run copy",
    "htmlhint": "htmlhint ./src/**/*.html -c ./.htmlhintrc && htmlhint ./demo/**/*.html -c ./.htmlhintrc",
    "lint": "npm run tslint && npm run htmlhint",
    "prerelease": "npm run lint && npm run build && npm run test",
    "publish": "cd dist && npm publish",
    "start": "webpack-dev-server --inline --progress --port 8080 --config=./config/webpack-demo.js",
    "test": "karma start",
    "tslint": "tslint ./src/**/*.ts -t verbose && tslint ./demo/**/*.ts -t verbose"
  },
  "author": "Charles Louw <charles.louw@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/cubitworx/angular-dsi-meteor.git"
  },
  "dependencies": {
    "@angular/common": "^4.0.2",
    "@angular/core": "^4.0.2",
    "@angular/http": "^4.0.2",
    "@angular/platform-browser": "^4.0.2",
    "@angular/platform-browser-dynamic": "^4.0.2",
    "@types/lodash": "^4.14.62",
    "@types/meteor": "^1.4.3",
    "@types/node": "^7.0.12",
    "babel-runtime": "^6.25.0",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "lodash": "^4.17.4",
    "meteor-rxjs": "^0.4.7",
    "ng2-bootstrap-modal": "^1.0.1",
    "node-sass": "^4.5.2",
    "reflect-metadata": "^0.1.10",
    "renamer": "^0.6.1",
    "rimraf": "^2.5.4",
    "rxjs": "^5.0.2",
    "typescript": "^2.2.1",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/animations": "^4.0.2",
    "@angular/compiler": "^4.0.2",
    "@angular/compiler-cli": "^4.0.2",
    "@angular/forms": "^4.3.2",
    "@angular/router": "^4.3.2",
    "@types/jasmine": "^2.5.47",
    "@types/webpack": "^2.2.8",
    "angular2-notifications": "^0.7.4",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.2",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.1",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "to-string-loader": "^1.1.5",
    "tslint": "^5.0.0",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.0"
  }
}
